---
import FooterLink from './FooterLink.astro';
import LangSwitch from '../components/LangSwitch.astro';

const {
  links,
} = Astro.props;
---

<footer>
  <LangSwitch />
  <ul>
    {links.map((link) => (
      <li><FooterLink link={link.link}>{link.text}</FooterLink></li>
    ))}
  </ul>
</footer>
